{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true,
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb/hooks"
    ],
    "parser": "babel-eslint", 
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", "jest"
    ],
    "rules": {
        "react/prop-types": ["error", { "ignore": ["navigation", "route"] }]
    }
}

